{
  "name": "azdevops-kube-summary-webapp-example",
  "version": "0.0.1",
  "description": "Sample webapp application using Kubernetes summary UI",
  "keywords": [
    "Azure DevOps",
    "Kubernetes"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/azpipelines-kubernetesUI-WebApp.git"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "main": "dist/server.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production && npm run copy-other ",
    "compiledev": "npm run clean && webpack --mode development && npm run copy-other ",
    "build": "npm run compile",
    "copy-other": "copyfiles -u 1 src/*.js dist",
    "start": "npm run compiledev && node dist/server.js"
  },
  "dependencies": {
    "@azurepipelines/azdevops-kube-summary": "^2.2.3",
    "@kubernetes/client-node": "^0.7.2",
    "azure-devops-ui": "~1.144.1",
    "express": "~4.16.4",
    "formidable": "~1.1.1",
    "office-ui-fabric-react": "5.131.5",
    "react": "~16.3.2",
    "react-dom": "~16.3.2"
  },
  "devDependencies": {
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "file-loader": "~2.0.0",
    "node": "~11.2.0",
    "node-inspect": "^1.11.5",
    "npm-sass": "^2.3.0",
    "rimraf": "~2.6.2",
    "sass": "^1.15.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
